{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-project017"
		},
		"ls_airflow_http_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_airflow_http'"
		},
		"ls_aws_s3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_aws_s3'"
		},
		"ls_blob_dev_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_dev'"
		},
		"ls_postgres_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_postgres'"
		},
		"ls_airflow_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://naval-intrinsic-archie.ngrok-free.dev"
		},
		"ls_airflow_http_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "airflow"
		},
		"ls_aws_s3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAQW3CPVFNAJBNBHAD"
		},
		"ls_postgres_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "airflow-postgres"
		},
		"ls_postgres_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "airflow"
		},
		"ls_postgres_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-project017_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_aws_s3_secretAccessKey": {
						"value": "[parameters('ls_aws_s3_secretAccessKey')]"
					},
					"ls_blob_dev_connectionString": {
						"value": "[parameters('ls_blob_dev_connectionString')]"
					},
					"ls_postgres_password": {
						"value": "[parameters('ls_postgres_password')]"
					},
					"ls_airflow_http_password": {
						"value": "[parameters('ls_airflow_http_password')]"
					},
					"ls_aws_s3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_aws_s3_properties_typeProperties_accessKeyId')]"
					},
					"ls_postgres_properties_typeProperties_server": {
						"value": "[parameters('ls_postgres_properties_typeProperties_server')]"
					},
					"ls_postgres_properties_typeProperties_database": {
						"value": "[parameters('ls_postgres_properties_typeProperties_database')]"
					},
					"ls_postgres_properties_typeProperties_username": {
						"value": "[parameters('ls_postgres_properties_typeProperties_username')]"
					},
					"ls_airflow_http_properties_typeProperties_url": {
						"value": "[parameters('ls_airflow_http_properties_typeProperties_url')]"
					},
					"ls_airflow_http_properties_typeProperties_userName": {
						"value": "[parameters('ls_airflow_http_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}